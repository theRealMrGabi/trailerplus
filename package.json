{
	"name": "TrailerPlus",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"start": "react-native start",
		"clean-start": "watchman watch-del-all && react-native start --reset-cache",
